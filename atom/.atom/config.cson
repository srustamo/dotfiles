"*":
  core:
    telemetryConsent: "no"
    themes: [
      "one-light-ui"
      "solarized-light-syntax"
    ]
  editor:
    fontFamily: "Hack"
    lineHeight: 1.3
    showIndentGuide: true
    zoomFontWhenCtrlScrolling: true
  "exception-reporting":
    userId: "6b25362c-ae5f-4261-906b-f9847dedb438"
  "spell-check": {}
  "tree-view":
    hideVcsIgnoredFiles: true
  "vim-mode-plus":
    smoothScrollOnFullScrollMotion: true
  welcome:
    showOnStartup: false
